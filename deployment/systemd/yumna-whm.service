[Unit]
Description=Yumna Control Plane (WHM)
After=network.target mysql.service

[Service]
Type=simple
User=root
WorkingDirectory=/opt/yumnapanel/whm
ExecStart=/usr/bin/npm start
Restart=always
Environment=NODE_ENV=production
EnvironmentFile=/opt/yumnapanel/whm/.env

[Install]
WantedBy=multi-user.target
